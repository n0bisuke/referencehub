{
  "name": "referencehub",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "d1:migrate": "wrangler d1 execute referencehub --remote --file=schema.sql",
    "d1:migrate:local": "wrangler d1 execute referencehub --local --file=schema.sql"
  },
  "dependencies": {
    "hono": "^4.10.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.2.3",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.5.1",
    "wrangler": "^4.17.0"
  }
}
